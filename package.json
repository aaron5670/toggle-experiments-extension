    "@mantine/hooks": "^5.9.5",
    "@plasmohq/storage": "^1.0.0",
    "@tabler/icons-react": "^2.1.2",
    "immer": "^9.0.15",
    "plasmo": "0.65.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "zustand": "^4.1.2"
  },
  "devDependencies": {
    "@plasmohq/prettier-plugin-sort-imports": "3.6.1",
