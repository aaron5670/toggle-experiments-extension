    "@mantine/hooks": "^6.0.2",
    "@plasmohq/storage": "^1.2.3",
    "@tabler/icons-react": "^2.10.0",
    "immer": "^9.0.19",
    "plasmo": "0.67.3",
    "react": "18.2.0",
